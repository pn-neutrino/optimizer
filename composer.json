{
  "name": "neutrino/optimizer",
  "description": "The Neutrino Optimizer package.",
  "keywords": ["neutrino", "nucleon", "composer", "optimize", "autoload"],
  "license": "MIT",
  "homepage": "https://phalcon-nucleon.github.io",
  "support": {
    "issues": "https://github.com/phalcon-nucleon/framework/issues",
    "source": "https://github.com/phalcon-nucleon/framework"
  },
  "authors": [
    {
      "name": "Ark4ne (Guillaume Allegret)",
      "email": "gui.allegret@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Neutrino\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/Test/"
    }
  },
  "minimum-stability": "dev"
}
